  "components": {
    "schemas": {
      "AdminProfile": {
        "type": "object",
        "properties": {
          "adminId": {
            "type": "integer",
            "format": "int32"
          },
          "department": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "permissionsLevel": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        },
        "additionalProperties": false
      },
      "AssignShipmentDto": {
        "required": [
          "driverId",
          "vehicleId"
        ],
        "type": "object",
        "properties": {
          "driverId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "vehicleId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordDto": {
        "required": [
          "newPassword",
          "oldPassword"
        ],
        "type": "object",
        "properties": {
          "oldPassword": {
            "minLength": 1,
            "type": "string"
          },
          "newPassword": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "City": {
        "required": [
          "cityName"
        ],
        "type": "object",
        "properties": {
          "cityId": {
            "type": "integer",
            "format": "int32"
          },
          "cityName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "region": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "users": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateDriverDto": {
        "required": [
          "email",
          "licenseNumber",
          "name",
          "password"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          },
          "phone": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "licenseNumber": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateOrUpdatePaymentDto": {
        "required": [
          "amount",
          "paymentMethod",
          "paymentStatus",
          "shipmentId"
        ],
        "type": "object",
        "properties": {
          "shipmentId": {
            "type": "integer",
            "format": "int32"
          },
          "amount": {
            "maximum": 1000000,
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "paymentMethod": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "paymentStatus": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreateRatingDto": {
        "required": [
          "ratingValue",
          "shipmentId"
        ],
        "type": "object",
        "properties": {
          "shipmentId": {
            "type": "integer",
            "format": "int32"
          },
          "ratingValue": {
            "maximum": 5,
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "comments": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateShipmentDto": {
        "required": [
          "destination",
          "origin"
        ],
        "type": "object",
        "properties": {
          "origin": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "destination": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "weight": {
            "maximum": 10000,
            "minimum": 0.1,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "priority": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "scheduledDate": {
            "type": "string",
            "format": "date",
            "nullable": true
          },
          "scheduledTime": {
            "type": "string",
            "format": "time",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateTrackingDto": {
        "required": [
          "location",
          "shipmentId"
        ],
        "type": "object",
        "properties": {
          "shipmentId": {
            "type": "integer",
            "format": "int32"
          },
          "location": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string",
